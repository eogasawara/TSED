@CONFERENCE{Perotti2023,
	author = {Perotti, Alan and Bajardi, Paolo and Bonchi, Francesco and Panisson, André},
	title = {Explaining Identity-aware Graph Classifiers through the Language of Motifs},
	year = {2023},
	journal = {Proceedings of the International Joint Conference on Neural Networks},
	volume = {2023-June},
	doi = {10.1109/IJCNN54540.2023.10191053},
