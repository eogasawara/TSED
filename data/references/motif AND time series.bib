@ARTICLE{Nguyen2010,
	author = {Nguyen, Tung T. and Almon, Richard R. and DuBois, Debra C. and Jusko, William J. and Androulakis, Ioannis P.},
	title = {Comparative analysis of acute and chronic corticosteroid pharmacogenomic effects in rat liver: Transcriptional dynamics and regulatory structures},
	year = {2010},
