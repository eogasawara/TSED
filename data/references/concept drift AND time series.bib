	title = {Machinery Anomaly Detection using artificial neural networks and signature feature extraction},
	year = {2023},
	journal = {Proceedings of the International Joint Conference on Neural Networks},
	volume = {2023-June},
	doi = {10.1109/IJCNN54540.2023.10191814},
