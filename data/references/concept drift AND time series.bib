@ARTICLE{Lang2022457,
	author = {Lang, Christopher I. and Sun, Fan-Keng and Lawler, Bruce and Dillon, Jack and Dujaili, Ash Al and Ruth, John and Cardillo, Peter and Alfred, Perry and Bowers, Alan and McKiernan, Adrian and Boning, Duane S.},
	title = {One Class Process Anomaly Detection Using Kernel Density Estimation Methods},
