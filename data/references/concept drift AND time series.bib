@ARTICLE{Wang202313186,
	author = {Wang, Shusen and Luo, Chao and Shao, Rui},
	title = {Unsupervised concept drift detection for time series on Riemannian manifolds},
	year = {2023},
	journal = {Journal of the Franklin Institute},
	volume = {360},
	number = {17},
	pages = {13186 – 13204},
	doi = {10.1016/j.jfranklin.2023.09.050},
