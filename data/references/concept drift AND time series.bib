@ARTICLE{Mirylenka2017661,
	author = {Mirylenka, Katsiaryna and Giannakopoulos, George and Do, Le Minh and Palpanas, Themis},
	title = {On classifier behavior in the presence of mislabeling noise},
	year = {2017},
	journal = {Data Mining and Knowledge Discovery},
	volume = {31},
